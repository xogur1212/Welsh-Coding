<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html lang="ko">
  <head>
    <meta charset="UTF-8" />
    <title>로그인 - IKEA</title>
    <link rel="stylesheet" href="gahee/css/pwSearch.css" />
    <script src="js/mainSearch.js"></script>
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta2/css/all.min.css"
    />
  </head>
  <body>
    <div id="wrap">
      <div class="left">
        <header>
          <i class="fas fa-arrow-left"></i>
          <h1 class="logo">
            <a href="#"><img src="gahee/images/loginLogo.png" alt="" /></a>
          </h1>
        </header>
        <!-- header 끝 -->
        <div id="content">
          <h1 class="login-title">비밀번호 찾기</h1>
          <p class="login-txt">이메일을 통해 비밀번호를 재설정 해주세요.</p>
        </div>
        <!-- content 끝 -->
        <footer>
          <span>IKEA.kr - </span><a href="#">개인정보처리방침</a>
          <p>© Inter IKEA Systems B.V. 1999-2021</p>
        </footer>
        <!-- footer 끝 -->
      </div>
      <!-- left 끝 -->
      <div class="right">
        <div class="login-id">
          <input type="text" name="" id="email" placeholder="이메일" />
        </div>
        <!-- login-id 끝 -->
        <a href="#" class="login-btn">비밀번호 재설정</a>
      </div>
      <!-- right 끝 -->
    </div>
  </body>
</html>
